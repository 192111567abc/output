#include<iostream>
using namespace std;
int main()
{
    int positive=0, negative=0, zero=0, i, arr[10];
    cout<<"Enter 10 Numbers: ";
    for(i=0; i<10; i++)
        cin>>arr[i];
    for(i=0; i<10; i++)
    {
        if(arr[i]>0)
            positive++;
        else if(arr[i]==0)
            zero++;
        else
            negative++;
    }
    cout<<"\nFrequency of Positive Numbers: "<<positive;
    cout<<"\nFrequency of Zero: "<<zero;
    cout<<"\nFrequency of Negative Numbers: "<<negative;
    cout<<endl;
    return 0;
}
